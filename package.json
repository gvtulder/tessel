{
  "name": "tilegame",
  "scripts": {
    "build": "NODE_ENV=production parcel build index.html --no-source-maps",
    "serve": "NODE_ENV=development parcel serve index.html --no-cache",
    "test": "jest",
    "check": "tsc --noEmit",
    "update-icon": "rsvg-convert icon.svg -w 180 -h 180 -o icon.png"
  },
  "browserslist": "defaults",
  "dependencies": {
    "@interactjs/actions": "^1.10.27",
    "@interactjs/auto-start": "^1.10.27",
    "@interactjs/interact": "^1.10.27",
    "@interactjs/pointer-events": "^1.10.27",
    "@parcel/service-worker": "^2.14.0",
    "detect-collisions": "^9.25.1",
    "interactjs": "^1.10.27",
    "polygon-clipping": "^0.15.7",
    "rbush": "^4.0.1",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "type": "module",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@parcel/packager-raw-url": "^2.14.0",
    "@parcel/reporter-bundle-analyzer": "^2.14.0",
    "@parcel/transformer-inline-string": "^2.14.0",
    "@parcel/transformer-sass": "^2.14.0",
    "@parcel/transformer-webmanifest": "^2.14.0",
    "@swc/jest": "^0.2.37",
    "babel-plugin-transform-define": "^2.1.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "color": "^5.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "^2.14.0",
    "parcel-namer-rewrite": "^2.10.3-rc.2",
    "prettier": "^3.5.3",
    "process": "^0.11.10",
    "svgo": "^3.3.2",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "parcel-namer-rewrite": {
    "hashing": "always",
    "disable": false,
    "rules": {
      "^([^s].*).js": "game.{hash}.js",
      "^(.*).css": "game.{hash}.css"
    }
  }
}
