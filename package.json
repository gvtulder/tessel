{
  "name": "tessel",
  "scripts": {
    "build": "NODE_ENV=production parcel build index.html --no-source-maps",
    "serve": "NODE_ENV=development parcel serve index.html --no-cache",
    "test": "jest",
    "check": "tsc --noEmit",
    "update-icon": "rsvg-convert icons/icon.svg -w 180 -h 180 -o icons/icon.png ; for s in 180 192 512 ; do rsvg-convert icons/icon.svg -w $s -h $s -o icons/icon-$s.png ; done",
    "screenshots": "tsx scripts/make-screenshots.ts"
  },
  "browserslist": "defaults",
  "dependencies": {
    "@parcel/service-worker": "^2.15.4",
    "detect-collisions": "^9.27.6",
    "polygon-clipping": "^0.15.7",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "type": "module",
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@parcel/packager-raw-url": "^2.15.4",
    "@parcel/reporter-bundle-analyzer": "^2.15.4",
    "@parcel/transformer-inline-string": "^2.15.4",
    "@parcel/transformer-sass": "^2.15.4",
    "@parcel/transformer-webmanifest": "^2.15.4",
    "@swc/jest": "^0.2.39",
    "babel-plugin-transform-define": "^2.1.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "color": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "parcel": "^2.15.4",
    "parcel-namer-rewrite": "^2.10.3-rc.5",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "svgo": "^4.0.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "parcel-namer-rewrite": {
    "hashing": "always",
    "disable": false,
    "rules": {
      "^([^s].*).js": "game.{hash}.js",
      "^(.*).css": "game.{hash}.css"
    }
  }
}
